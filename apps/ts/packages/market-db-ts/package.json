{
  "name": "@trading25/market-db-ts",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.ts"
    },
    "./db": {
      "import": "./dist/db/index.js",
      "types": "./dist/db/index.d.ts"
    },
    "./db/*": {
      "import": "./dist/db/*.js",
      "types": "./dist/db/*.d.ts"
    },
    "./dataset/*": {
      "import": "./dist/dataset/*.js",
      "types": "./dist/dataset/*.d.ts"
    },
    "./portfolio/*": {
      "import": "./dist/portfolio/*.js",
      "types": "./dist/portfolio/*.d.ts"
    },
    "./watchlist/*": {
      "import": "./dist/watchlist/*.js",
      "types": "./dist/watchlist/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit --rootDir ../.."
  },
  "dependencies": {
    "drizzle-orm": "^0.44.0"
  }
}
